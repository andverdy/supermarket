<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	
	<servlet>
		<servlet-name>getArticles</servlet-name>
		<servlet-class>it.objectmethod.supermarket.servlets.ArticlesGetServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>getArticles</servlet-name>
		<url-pattern>/GetArticles</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>insArticle</servlet-name>
		<servlet-class>it.objectmethod.supermarket.servlets.ArticleInsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>insArticle</servlet-name>
		<url-pattern>/InsArticle</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>formOneArticles</servlet-name>
		<servlet-class>it.objectmethod.supermarket.servlets.FormViewServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>formOneArticles</servlet-name>
		<url-pattern>/FormViewServ</url-pattern>
	</servlet-mapping>
	
</web-app>
